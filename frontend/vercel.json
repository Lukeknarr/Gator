{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://gator-api.up.railway.app"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://gator-api.up.railway.app/:path*"
    }
  ]
} 